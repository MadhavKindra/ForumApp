<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:onClick="preBtnClicked"
    tools:context=".Activities.Home2Activity">

    <TextView
        android:id="@+id/home5"
        android:layout_width="265dp"
        android:layout_height="41dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/reg_edittext_style"
        android:onClick="h10Clicked"
        android:text="ZENITH-Pink Legal Club Quarterly Fest"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home4" />

    <TextView
        android:id="@+id/home4"
        android:layout_width="265dp"
        android:layout_height="41dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/reg_edittext_style"
        android:onClick="h9Clicked"
        android:text="Apti-Holic, All India Aptitude Test"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home3" />

    <TextView
        android:id="@+id/home2"
        android:layout_width="265dp"
        android:layout_height="41dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/reg_edittext_style"
        android:onClick="h7Clicked"
        android:text="Job opportunity at EY"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home1" />

    <TextView
        android:id="@+id/home3"
        android:layout_width="265dp"
        android:layout_height="41dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/reg_edittext_style"
        android:onClick="h8Clicked"
        android:text="Internship Opportunity at FlytBase"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home2" />

    <TextView
        android:id="@+id/home1"
        android:layout_width="265dp"
        android:layout_height="41dp"
        android:layout_marginTop="44dp"
        android:background="@drawable/reg_edittext_style"
        android:onClick="h6Clicked"
        android:text="Internship Opportunity at Logyify"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="71dp"
        android:layout_marginTop="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <Button
        android:id="@+id/home1Btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/reg_btn_style"
        android:onClick="preBtnClicked"
        android:text="Previous"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home5" />

</androidx.constraintlayout.widget.ConstraintLayout>